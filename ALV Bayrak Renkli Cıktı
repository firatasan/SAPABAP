* 1
  LOOP AT lt_bayrak INTO ls_bayrak.
    ld_index = sy-tabix.
*   Set colour of EBELN field to various colors based on sy-tabix value
    wa_cellcolor-fname = 'bir'.

    IF ls_bayrak-bir EQ 'Kirmizi'.
      wa_cellcolor-color-col = '6'.  "color code 1-7, if outside rage defaults to 7
      wa_cellcolor-color-int = '1'.  "1 = Intensified on, 0 = Intensified off
      wa_cellcolor-color-inv = '0'.  "1 = text colour, 0 = background colour
      APPEND wa_cellcolor TO ls_bayrak-cellcolor.
      MODIFY lt_bayrak FROM ls_bayrak INDEX ld_index TRANSPORTING cellcolor.

    ELSE.

      wa_cellcolor-color-col = '0'.  "color code 1-7, if outside rage defaults to 7
      wa_cellcolor-color-int = '1'.  "1 = Intensified on, 0 = Intensified off
      wa_cellcolor-color-inv = '0'.  "1 = text colour, 0 = background colour
      APPEND wa_cellcolor TO ls_bayrak-cellcolor.
      MODIFY lt_bayrak FROM ls_bayrak INDEX ld_index TRANSPORTING cellcolor.

    ENDIF.

*    WA_CELLCOLOR-COLOR-COL = '6'.  "color code 1-7, if outside rage defaults to 7
*    WA_CELLCOLOR-COLOR-INT = '1'.  "1 = Intensified on, 0 = Intensified off
*    WA_CELLCOLOR-COLOR-INV = '0'.  "1 = text colour, 0 = background colour
*    APPEND WA_CELLCOLOR TO ls_bayrak-CELLCOLOR.
*    MODIFY lt_bayrak from ls_bayrak INDEX ld_index TRANSPORTING CELLCOLOR.
